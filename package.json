{
    "name": "code-snippets-helper",
    "displayName": "Code Snippets Helper",
    "description": "Quick snippets for common data structures",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Snippets"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "code-snippets-helper.addSnippet",
                "title": "Add Code Snippet"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts"
    },
    "devDependencies": {
        "@types/node": "^16.18.34",
        "@types/vscode": "^1.97.0",
        "@typescript-eslint/eslint-plugin": "^6.7.0",
        "@typescript-eslint/parser": "^6.7.0",
        "eslint": "^8.47.0",
        "typescript": "^5.2.2"
    }
}